<resources>

    <declare-styleable name="DoubleText">
        <attr name="topText" format="string" />
        <attr name="bottomText" format="string" />
        <attr name="android:ems" />

    </declare-styleable>

    <declare-styleable name="customDrawablesAttr">
        <attr name="cardBackground" format="color" />
    </declare-styleable>
    <declare-styleable name="customDrawableLinesAttr">
        <attr name="lines" format="color" />
        <attr name="whiteLines" format="color" />
        <attr name="whiteLinesCompass" format="color" />
    </declare-styleable>

    <declare-styleable name="customDrawablesHighlightAttr">
        <attr name="highlight" format="color" />
    </declare-styleable>

    <declare-styleable name="customDrawablesArabicNameAttr">
        <attr name="arabicName" format="color" />
    </declare-styleable>

    <declare-styleable name="customCardLines">
        <attr name="dividerLight" format="color" />
        <attr name="dividerDark" format="color" />
    </declare-styleable>

</resources>